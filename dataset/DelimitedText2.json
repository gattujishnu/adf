{
	"name": "DelimitedText2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureGen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{dataset().filename}{formatDateTime(pipeline.parameters.triggerstart, 'yyyyMMddHHmm')}.csv",
					"type": "Expression"
				},
				"folderPath": "customerData",
				"fileSystem": "web"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "CustomerID",
				"type": "String"
			},
			{
				"name": "Name",
				"type": "String"
			},
			{
				"name": "Email",
				"type": "String"
			},
			{
				"name": "Phone",
				"type": "String"
			},
			{
				"name": "Address",
				"type": "String"
			},
			{
				"name": "City",
				"type": "String"
			},
			{
				"name": "State",
				"type": "String"
			},
			{
				"name": "ZipCode",
				"type": "String"
			},
			{
				"name": "LastUpdated",
				"type": "String"
			}
		]
	}
}