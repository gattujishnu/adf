		},
		"AzureSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSql'"
		},
		"Validation_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Validation'"
		},
		"AzureGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://varungattu.dfs.core.windows.net/"
