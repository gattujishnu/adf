{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "varungattudf"
		},
		"AzureGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureGen2'"
		},
		"AzureSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSql'"
		},
		"Validation_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Validation'"
		},
		"AzureGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://varungattu.dfs.core.windows.net/"
		},
		"AzureSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "varun.database.windows.net"
		},
		"AzureSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "jishnu"
		},
		"AzureSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "jishnu"
		},
		"Validation_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://varungattu.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "varungattudf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureGen2_accountKey": {
						"value": "[parameters('AzureGen2_accountKey')]"
					},
					"AzureSql_password": {
						"value": "[parameters('AzureSql_password')]"
					},
					"Validation_accountKey": {
						"value": "[parameters('Validation_accountKey')]"
					},
					"AzureGen2_properties_typeProperties_url": {
						"value": "[parameters('AzureGen2_properties_typeProperties_url')]"
					},
					"AzureSql_properties_typeProperties_server": {
						"value": "[parameters('AzureSql_properties_typeProperties_server')]"
					},
					"AzureSql_properties_typeProperties_database": {
						"value": "[parameters('AzureSql_properties_typeProperties_database')]"
					},
					"AzureSql_properties_typeProperties_userName": {
						"value": "[parameters('AzureSql_properties_typeProperties_userName')]"
					},
					"Validation_properties_typeProperties_url": {
						"value": "[parameters('Validation_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "varungattudf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'varungattudf_ArmTemplate_0')]"
			]
		}
	]
}